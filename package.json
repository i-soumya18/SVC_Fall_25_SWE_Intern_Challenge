{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "node dist/server/node-build.mjs",
    "pretest": "npm run test:db:setup",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "vitest --config vitest.config.backend.ts --run",
    "test:backend:watch": "vitest --config vitest.config.backend.ts",
    "test:backend:coverage": "vitest --config vitest.config.backend.ts --coverage --run",
    "test:frontend": "vitest --config vitest.config.ts --run",
    "test:frontend:coverage": "vitest --config vitest.config.ts --coverage --run",
    "posttest": "npm run test:db:clean",
    "test:all": "npm run test && npm run test:backend",
    "test:coverage": "npm run test:backend:coverage",
    "test:db:setup": "docker run --name fairdatause-test-db -e POSTGRES_DB=fairdatause_test -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -p 5432:5432 -d postgres:15",
    "test:db:start": "docker start fairdatause-test-db",
    "test:db:stop": "docker stop fairdatause-test-db",
    "test:db:clean": "docker rm -f fairdatause-test-db",
    "test:db:logs": "docker logs fairdatause-test-db",
    "test:unit": "vitest run tests/buffer-parsing.test.ts",
    "format.fix": "prettier --write .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.7",
