# Environment variables for the application
# Copy this file to .env and fill in the values

# Builder.io public key (get from https://www.builder.io/c/docs/using-your-api-key)
VITE_PUBLIC_BUILDER_KEY=your_builder_public_key_here

# Ping message for testing
PING_MESSAGE="ping pong"

# Supabase magic link redirect URL
VITE_SITE_URL=http://localhost:8080

# Database URL (for production, use a real PostgreSQL database)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/fairdatause

# Reddit API credentials (get from https://www.reddit.com/prefs/apps)
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here